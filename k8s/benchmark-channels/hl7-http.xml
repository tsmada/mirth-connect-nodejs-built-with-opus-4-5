        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>Extract HL7 Fields</name>
          <sequenceNumber>0</sequenceNumber>
          <enabled>true</enabled>
          <script>var msgType = msg['MSH']['MSH.9']['MSH.9.1'].toString();
var triggerEvent = msg['MSH']['MSH.9']['MSH.9.2'].toString();
var sendingApp = msg['MSH']['MSH.3']['MSH.3.1'].toString();
var sendingFacility = msg['MSH']['MSH.4']['MSH.4.1'].toString();
var messageControlId = msg['MSH']['MSH.10'].toString();

var patientId = '';
var lastName = '';
var firstName = '';
var dob = '';
var gender = '';

try {
  patientId = msg['PID']['PID.3']['PID.3.1'].toString();
  lastName = msg['PID']['PID.5']['PID.5.1'].toString();
  firstName = msg['PID']['PID.5']['PID.5.2'].toString();
  dob = msg['PID']['PID.7']['PID.7.1'].toString();
  gender = msg['PID']['PID.8'].toString();
} catch (e) {
  /* PID segment may not be present in all message types */
}

$c('messageType', msgType + '^' + triggerEvent);
$c('patientId', patientId);
$c('patientName', firstName + ' ' + lastName);
$c('sendingApp', sendingApp);
$c('sendingFacility', sendingFacility);
$c('messageControlId', messageControlId);</script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
