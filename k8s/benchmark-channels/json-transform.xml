        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>JSON to XML Transform</name>
          <sequenceNumber>0</sequenceNumber>
          <enabled>true</enabled>
          <script>var patient = JSON.parse(connectorMessage.getRawData());

var givenName = patient.name &amp;&amp; patient.name.given ? patient.name.given : '';
var familyName = patient.name &amp;&amp; patient.name.family ? patient.name.family : '';
var patientId = patient.id || 'UNKNOWN';
var dob = patient.birthDate || '';
var gender = patient.gender || 'unknown';

var addresses = patient.address || [];
var addrXml = '';
for (var i = 0; i &lt; addresses.length; i++) {
  var a = addresses[i];
  addrXml += '&lt;address use="' + (a.use || 'home') + '"&gt;';
  addrXml += '&lt;line&gt;' + (a.line || '') + '&lt;/line&gt;';
  addrXml += '&lt;city&gt;' + (a.city || '') + '&lt;/city&gt;';
  addrXml += '&lt;state&gt;' + (a.state || '') + '&lt;/state&gt;';
  addrXml += '&lt;postalCode&gt;' + (a.postalCode || '') + '&lt;/postalCode&gt;';
  addrXml += '&lt;/address&gt;';
}

var telecoms = patient.telecom || [];
var teleXml = '';
for (var j = 0; j &lt; telecoms.length; j++) {
  var t = telecoms[j];
  teleXml += '&lt;telecom system="' + (t.system || 'phone') + '" use="' + (t.use || 'home') + '"&gt;';
  teleXml += (t.value || '');
  teleXml += '&lt;/telecom&gt;';
}

var result = '&lt;Patient xmlns="urn:hl7-org:fhir"&gt;';
result += '&lt;id value="' + patientId + '"/&gt;';
result += '&lt;name&gt;&lt;given value="' + givenName + '"/&gt;&lt;family value="' + familyName + '"/&gt;&lt;/name&gt;';
result += '&lt;gender value="' + gender + '"/&gt;';
result += '&lt;birthDate value="' + dob + '"/&gt;';
result += addrXml;
result += teleXml;
result += '&lt;meta&gt;&lt;lastUpdated value="' + new Date().toISOString() + '"/&gt;&lt;/meta&gt;';
result += '&lt;/Patient&gt;';

$c('transformedXml', result);
$c('patientId', patientId);</script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
      </elements>
