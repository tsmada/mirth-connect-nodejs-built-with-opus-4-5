          <sequenceNumber>0</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[// Parse JSON input
var inputStr = connectorMessage.getRawData();
var input = JSON.parse(inputStr);

// Build XML using E4X-style patterns (via code template)
var patientXml = buildPatientXml(
  input.name || {},
  (input.identifiers && input.identifiers[0]) ? input.identifiers[0].value : '',
  input.gender || '',
  input.age || 0
);

// Extract identifiers using code template
var ids = extractIdentifiers(input.identifiers || []);
$c('identifierMap', JSON.stringify(ids));
$c('patientMRN', ids['MRN'] || '');

// Routing logic based on age and gender
var routeKey = 'C'; // default
if (input.gender === 'female' && input.age < 40) {
  routeKey = 'A';
} else if (input.gender === 'male') {
  routeKey = 'B';
}
$c('routeKey', routeKey);
$c('patientName', (input.name && input.name.family) ? input.name.family + ', ' + (input.name.given || '') : 'UNKNOWN');
$c('patientXml', patientXml.toString());

var total = ($gc('dv02TotalReceived') || 0) + 1;
$gc('dv02TotalReceived', total);]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
