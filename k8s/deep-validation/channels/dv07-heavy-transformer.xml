          <script><![CDATA[// E4X patterns: for loop, descendants, attribute operations
var identifierMap = {};
for (var idx = 0; idx < msg['PID']['PID.3'].length(); idx++) {
  var idValue = msg['PID']['PID.3'][idx]['PID.3.1'].toString();
  var idType = msg['PID']['PID.3'][idx]['PID.3.4'].toString();
  identifierMap[idType] = idValue;
}
var mrn = identifierMap['MRN'] || '';

// Use all 6 map types
$c('patientMRN', mrn);
$s('sourceChannelName', 'DV07');
$gc('dv07LastMRN', mrn);
var globalCount = ($g('dv07ProcessedCount') || 0);
$g('dv07ProcessedCount', globalCount);
// $cfg is read-only in transformer, just read it
var cfgVal = $cfg('dv07ConfigKey') || 'default';
$co('connectorProcessed', 'true');

// Code template calls
var enrichResult = lookupPatientByMRN(mrn);
var eventType = msg['MSH']['MSH.9']['MSH.9.2'].toString();
var eventDesc = buildEventDescription(eventType, $c('patientMRN'), 'DV_HEAVY');
$c('eventType', eventType);
$c('eventDesc', eventDesc);
$c('enrichResult', JSON.stringify(enrichResult));

// Lists and Maps utilities
var tagList = Lists.list('DV07', 'HEAVY', eventType);
$c('tagList', tagList.toString());
var propMap = Maps.map('channel', 'DV07', 'type', eventType, 'mrn', mrn);
$c('propMap', JSON.stringify(propMap));

// DatabaseConnection usage
try {
  var dbConn = DatabaseConnectionFactory.createDatabaseConnection(
    'com.mysql.cj.jdbc.Driver',
    'jdbc:mysql://mysql.mirth-infra:3306/mirthdb',
    'mirth', 'mirth'
  );
  var result = dbConn.executeCachedQuery("SELECT COUNT(*) AS cnt FROM dv_batch_results WHERE patient_id = '" + mrn + "'");
  if (result.next()) {
    $c('priorBatchCount', String(result.getInt('cnt')));
  }
  dbConn.close();
} catch (e) {
  $c('priorBatchCount', 'db_error: ' + e.message);
}

// DateUtil usage
var dateStr = msg['PID']['PID.7']['PID.7.1'].toString();
$c('formattedDOB', formatMirthDate(dateStr));]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
