{
  "name": "mirth-connect-nodejs",
  "version": "0.1.0",
  "description": "Node.js/TypeScript runtime replacement for Mirth Connect Java engine",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mirth-cli": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "cli": "ts-node src/cli/index.ts",
    "cli:build": "tsc && chmod +x dist/cli/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts --ext .tsx",
    "lint:fix": "eslint src --ext .ts --ext .tsx --fix",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"src/**/*.tsx\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docker:up": "docker compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker compose -f docker/docker-compose.yml down",
    "docker:logs": "docker compose -f docker/docker-compose.yml logs -f",
    "tech-debt": "npm run lint && npm outdated",
    "tech-debt:dupes": "jscpd src --reporters console",
    "tech-debt:unused": "knip --reporter compact"
  },
  "keywords": [
    "mirth",
    "mirth-connect",
    "hl7",
    "healthcare",
    "integration-engine"
  ],
  "author": "",
  "license": "MPL-2.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.454.0",
    "axios": "^1.6.2",
    "basic-ftp": "^5.0.4",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "conf": "^12.0.0",
    "date-fns": "^3.6.0",
    "express": "^4.18.2",
    "fast-xml-parser": "^4.5.3",
    "ink": "^4.4.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^7.0.13",
    "ora": "^8.0.1",
    "react": "^18.2.0",
    "ssh2-sftp-client": "^9.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^7.0.9",
    "@types/react": "^18.2.0",
    "@types/ssh2-sftp-client": "^9.0.6",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jscpd": "^4.0.5",
    "knip": "^5.41.1",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
