          <name>ORM to ORU Transformer</name>
          <sequenceNumber>0</sequenceNumber>
          <enabled>true</enabled>
          <script>
// ORM^O01 to ORU^R01 Transformer
// Reads ORM lab order and produces ORU with mock lab results

var rawMsg = messageObject.getRawData();
var segments = rawMsg.split('\n');
var oru = '';
var obrCount = 0;

// Mock lab results keyed by CPT code
var mockResults = {
  '80053': [
    { code: '2345-7', name: 'Glucose', value: '95', units: 'mg/dL', range: '70-100', flag: 'N' },
    { code: '3094-0', name: 'BUN', value: '15', units: 'mg/dL', range: '7-20', flag: 'N' },
    { code: '2160-0', name: 'Creatinine', value: '1.0', units: 'mg/dL', range: '0.7-1.3', flag: 'N' }
  ],
  '85025': [
    { code: '6690-2', name: 'WBC', value: '7.5', units: '10*3/uL', range: '4.5-11.0', flag: 'N' },
    { code: '789-8', name: 'RBC', value: '4.8', units: '10*6/uL', range: '4.5-5.5', flag: 'N' },
    { code: '718-7', name: 'Hemoglobin', value: '14.2', units: 'g/dL', range: '12.0-16.0', flag: 'N' }
  ]
};

for (var i = 0; i &lt; segments.length; i++) {
  var seg = segments[i].replace(/\r/g, '');
  if (seg.length === 0) continue;

  var fields = seg.split('|');
  var segName = fields[0];

  if (segName === 'MSH') {
    // Build ORU MSH: swap sender/receiver, change message type
    oru += 'MSH|^~\\&amp;|LAB_SYS|LAB_A|ORDER_SYS|CLINIC_A|' +
           '20260207130000||ORU^R01^ORU_R01|RESULT001|P|2.5\r';
  } else if (segName === 'PID') {
    oru += seg + '\r';
  } else if (segName === 'PV1') {
    oru += seg + '\r';
  } else if (segName === 'ORC') {
    // Change order control from NW (new) to RE (results)
    fields[1] = 'RE';
    oru += fields.join('|') + '\r';
  } else if (segName === 'OBR') {
    obrCount++;
    // Add result status F (final) to OBR-25
    while (fields.length &lt; 26) fields.push('');
    fields[25] = 'F';
    oru += fields.join('|') + '\r';

    // Extract CPT code from OBR-4 (e.g., "80053^COMPREHENSIVE METABOLIC PANEL^CPT")
    var obr4 = fields[4] || '';
    var cptCode = obr4.split('^')[0];

    // Add mock OBX results for this order
    var results = mockResults[cptCode] || [];
    for (var j = 0; j &lt; results.length; j++) {
      var r = results[j];
      var setId = j + 1;
      oru += 'OBX|' + setId + '|NM|' + r.code + '^' + r.name + '||' +
             r.value + '|' + r.units + '|' + r.range + '|' + r.flag +
             '|||F\r';
    }
  }
}

// Set transformed message as raw string
tmp['msg'] = oru;
          </script>
