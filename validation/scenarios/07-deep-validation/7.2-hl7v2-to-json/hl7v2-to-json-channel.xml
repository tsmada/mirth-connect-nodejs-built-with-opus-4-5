          <script>// Determine message type and extract accordingly
var msgType = msg['MSH']['MSH.9']['MSH.9.1'].toString();
var result;

if (msgType === 'ADT') {
  // Extract patient demographics from ADT
  result = {
    mrn: msg['PID']['PID.3']['PID.3.1'].toString(),
    lastName: msg['PID']['PID.5']['PID.5.1'].toString(),
    firstName: msg['PID']['PID.5']['PID.5.2'].toString(),
    middleName: msg['PID']['PID.5']['PID.5.3'].toString() || '',
    dob: msg['PID']['PID.7']['PID.7.1'].toString(),
    gender: msg['PID']['PID.8']['PID.8.1'].toString(),
    address: {
      street: msg['PID']['PID.11']['PID.11.1'].toString(),
      city: msg['PID']['PID.11']['PID.11.3'].toString(),
      state: msg['PID']['PID.11']['PID.11.4'].toString(),
      zip: msg['PID']['PID.11']['PID.11.5'].toString()
    },
    phone: msg['PID']['PID.13']['PID.13.1'].toString(),
    ssn: msg['PID']['PID.19']['PID.19.1'].toString() || ''
  };

  // Store in channel map for validation
  $c('extractedPatient', result);

} else if (msgType === 'ORU') {
  // Extract lab results from ORU
  var obxSegments = msg['OBX'];
  var results = [];

  // Handle single OBX or multiple OBX segments
  var obxList = obxSegments.length ? obxSegments : [obxSegments];

  for (var i = 0; i &lt; obxList.length(); i++) {
    var obx = obxList[i];
    results.push({
      code: obx['OBX.3']['OBX.3.1'].toString(),
      name: obx['OBX.3']['OBX.3.2'].toString(),
      value: parseFloat(obx['OBX.5']['OBX.5.1'].toString()) || obx['OBX.5']['OBX.5.1'].toString(),
      units: obx['OBX.6']['OBX.6.1'].toString(),
      referenceRange: obx['OBX.7']['OBX.7.1'].toString(),
      flag: obx['OBX.8']['OBX.8.1'].toString(),
      status: obx['OBX.11']['OBX.11.1'].toString()
    });
  }

  result = {
    patient: {
      mrn: msg['PID']['PID.3']['PID.3.1'].toString(),
      name: msg['PID']['PID.5'].toString()
    },
    order: {
      orderId: msg['OBR']['OBR.2']['OBR.2.1'].toString(),
      specimenId: msg['OBR']['OBR.3']['OBR.3.1'].toString(),
      testCode: msg['OBR']['OBR.4']['OBR.4.1'].toString(),
      testName: msg['OBR']['OBR.4']['OBR.4.2'].toString(),
      orderingProvider: msg['OBR']['OBR.16'].toString(),
      collectionTime: msg['OBR']['OBR.7']['OBR.7.1'].toString(),
      resultTime: msg['OBR']['OBR.22']['OBR.22.1'].toString(),
      status: msg['OBR']['OBR.25']['OBR.25.1'].toString()
    },
    results: results
  };

  // Store in channel map for validation
  $c('extractedLabResults', result);
}

// Set output as JSON string
tmp = JSON.stringify(result, null, 2);</script>
