          <sequenceNumber>0</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[// TRANSPILER PASS 3: Descendant operator (..)
var pidNode = msg..PID;
var allOBX = msg..OBX;

// XMLPROXY: Bracket-quote dotted path navigation
var familyName = msg['PID']['PID.5'][0]['PID.5.1'].toString();
var givenName = msg['PID']['PID.5'][0]['PID.5.2'].toString();
var pid3_1 = msg['PID']['PID.3'][0]['PID.3.1'].toString();

// XMLPROXY: length() on repeating fields
var obxCount = msg['OBX'].length();
var pid3Count = msg['PID']['PID.3'].length();

// XMLPROXY: hasSimpleContent() / hasComplexContent()
var leafIsSimple = msg['PID']['PID.5'][0]['PID.5.1'].hasSimpleContent();
var branchIsComplex = msg['PID']['PID.5'][0].hasComplexContent();

$c('familyName', familyName);
$c('givenName', givenName);
$c('patientId', pid3_1);
$c('obxCountBefore', String(obxCount));
$c('pid3Count', String(pid3Count));
$c('leafIsSimple', String(leafIsSimple));
$c('branchIsComplex', String(branchIsComplex));]]></script>
