        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>For-Each Loop + Repeating Field Iteration</name>
          <sequenceNumber>1</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[// TRANSPILER PASS 2: for-each with var declaration
var obxValues = [];
for each (var obx in msg..OBX) {
  var code = obx['OBX.3']['OBX.3.1'].toString();
  var value = obx['OBX.5']['OBX.5.1'].toString();
  obxValues.push(code + '=' + value);
}
$c('obxValueList', obxValues.join('|'));

// TRANSPILER PASS 2: for-each with bare variable
var segNames = [];
var seg;
for each (seg in msg.children()) {
  segNames.push(seg.name().toString());
}
$c('segmentNames', segNames.join(','));

// Standard index-based iteration over repeating PID.3
var identifiers = [];
for (var i = 0; i < msg['PID']['PID.3'].length(); i++) {
  var id = msg['PID']['PID.3'][i]['PID.3.1'].toString();
  var type = msg['PID']['PID.3'][i]['PID.3.4'].toString();
  identifiers.push(type + ':' + id);
}
$c('allIdentifiers', identifiers.join('|'));]]></script>
