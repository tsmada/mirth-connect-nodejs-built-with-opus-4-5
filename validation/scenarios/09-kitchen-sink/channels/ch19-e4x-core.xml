          <name>Backward Iteration + Delete Operations</name>
          <sequenceNumber>2</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[// Delete specific named segment
delete msg['NTE'];

// XMLPROXY: removeChild via bracket delete
delete msg['PID']['PID.6'];

// Backward iteration with conditional delete
for (var j = msg['OBX'].length() - 1; j >= 0; j--) {
  var obxCode = msg['OBX'][j]['OBX.3']['OBX.3.1'].toString();
  if (obxCode === 'SODIUM') {
    delete msg['OBX'][j];
  }
}

var obxCountAfter = msg['OBX'].length();
$c('obxCountAfter', String(obxCountAfter));
$c('nteDeleted', String(typeof msg['NTE'] === 'undefined' || msg['NTE'].length() === 0));
$c('pid6Deleted', String(msg['PID']['PID.6'].toString() === ''));]]></script>
