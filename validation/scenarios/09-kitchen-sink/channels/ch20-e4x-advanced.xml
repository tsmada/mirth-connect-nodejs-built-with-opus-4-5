          <name>Attribute Read + Write</name>
          <sequenceNumber>0</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[try {
  // TRANSPILER PASS 5 (write): .@attr = value
  msg['MSH'].@customSource = 'E4X_ADVANCED';
  msg['MSH'].@processedBy = 'NodeJS_Mirth';

  // TRANSPILER PASS 5 (read): .@attr
  var customSrc = msg['MSH'].@customSource;
  $c('attrWrite', customSrc.toString());

  // TRANSPILER PASS 3.5 (wildcard): .@*
  var allAttrs = msg['MSH'].@*;
  var attrKeys = allAttrs ? (typeof allAttrs === 'object' ? Object.keys(allAttrs) : []) : [];
  $c('attrCount', String(attrKeys.length));

  // Attribute on deeper element
  msg['PID']['PID.5'][0].@encoding = 'UTF-8';
  var enc = msg['PID']['PID.5'][0].@encoding;
  $c('deepAttrRead', enc.toString());

  // Attribute equality check
  var isCustom = (msg['MSH'].@customSource == 'E4X_ADVANCED');
  $c('attrEquality', String(isCustom));
} catch(e) {
  $c('attrWrite', 'step0_error: ' + e.message);
  $c('attrCount', '0');
  $c('deepAttrRead', 'error');
  $c('attrEquality', 'false');
}]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>XML Literals + new XML() + XMLList</name>
          <sequenceNumber>1</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[try {
  // TRANSPILER PASS 4: XML literal
  var emptyNode = <ZAD/>;

  // TRANSPILER PASS 4: XML literal with content
  var richNode = <ZAD><ZAD.1>ADVANCED_TEST</ZAD.1><ZAD.2>literal_value</ZAD.2></ZAD>;

  // TRANSPILER PASS 6: new XML() constructor
  var constructed = new XML('<ZAD><ZAD.3>constructed_value</ZAD.3></ZAD>');

  // TRANSPILER PASS 6: XMLList constructor
  var xmlListStr = msg['OBX'].toXMLString();
  var obxList = new XMLList(xmlListStr);
  $c('xmlListLength', String(obxList.length()));

  // XMLPROXY: toXMLString()
  var pidXml = msg['PID'].toXMLString();
  $c('pidXmlLength', String(pidXml.length));
  $c('hasXmlLiteral', 'true');
} catch(e) {
  $c('xmlListLength', '0');
  $c('pidXmlLength', '0');
  $c('hasXmlLiteral', 'step1_error: ' + e.message);
}]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>XML Append Operator (+= variants)</name>
          <sequenceNumber>2</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[// TRANSPILER PASS 5.5 Rule 1: += XMLProxy.create()
msg += XMLProxy.create('<ZAP><ZAP.1>appended_via_create</ZAP.1></ZAP>');

// TRANSPILER PASS 5.5 Rule 2: += variable
var newSeg = new XML('<ZAV><ZAV.1>appended_via_var</ZAV.1></ZAV>');
msg += newSeg;

// Verify both appended segments exist
var msgStr = msg.toString();
$c('hasZAP', String(msgStr.indexOf('ZAP') >= 0));
$c('hasZAV', String(msgStr.indexOf('ZAV') >= 0));

// Bracket assignment for child append (ZKS may or may not be in VM-forwarded message)
try {
  if (msg['ZKS'] && msg['ZKS'].length() > 0) {
    msg['ZKS']['ZKS.5'] = 'appended_to_existing';
    $c('zks5Value', msg['ZKS']['ZKS.5'].toString());
  } else {
    // Create ZKS fresh if not present from CH19
    createSegment('ZKS', msg);
    msg['ZKS']['ZKS.1'] = 'CreatedInCH20';
    msg['ZKS']['ZKS.5'] = 'appended_to_existing';
    $c('zks5Value', 'appended_to_existing');
  }
} catch(e) {
  $c('zks5Value', 'zks_access_error');
}]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>Default XML Namespace + namespace()</name>
          <sequenceNumber>3</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[try {
  // TRANSPILER PASS 1: default xml namespace
  default xml namespace = 'urn:hl7-org:v3';
} catch(e) {
  // default xml namespace may not be supported — that's OK
}

// XMLPROXY: namespace() extraction
try {
  var cdaNode = new XML('<ClinicalDocument xmlns="urn:hl7-org:v3"><id root="2.16.840"/></ClinicalDocument>');
  var ns = cdaNode.namespace();
  $c('defaultNs', ns ? ns.toString() : 'none');
} catch(e) {
  $c('defaultNs', 'ns_error: ' + e.message);
}

// XMLPROXY: namespace with prefix
try {
  var nsNode = new XML('<soap:Envelope xmlns:soap="http://schemas.xmlsoap.org/soap/envelope/"><soap:Body/></soap:Envelope>');
  var soapNs = nsNode.namespace('soap');
  $c('prefixedNs', soapNs ? soapNs.toString() : 'none');
} catch(e) {
  $c('prefixedNs', 'soap_ns_error: ' + e.message);
}

// Reset namespace
try { default xml namespace = ''; } catch(e) { /* OK */ }

$c('e4xAdvancedProcessed', 'true');
$s('sourceType', 'E4X_ADVANCED');]]></script>
