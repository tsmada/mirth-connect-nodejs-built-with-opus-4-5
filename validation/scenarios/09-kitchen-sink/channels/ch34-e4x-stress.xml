          <name>Multi-PID.3 Identifier Extraction</name>
          <sequenceNumber>0</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[var identifierMap = {};
for (var idx = 0; idx < msg['PID']['PID.3'].length(); idx++) {
  var idValue = msg['PID']['PID.3'][idx]['PID.3.1'].toString();
  var idType = msg['PID']['PID.3'][idx]['PID.3.4'].toString();
  identifierMap[idType] = idValue;
}
$c('identifierMap', JSON.stringify(identifierMap));
$c('hasMRN', String(identifierMap['MRN'] !== undefined));
$c('hasSSN', String(identifierMap['SSN'] !== undefined));]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>OBX Lab Routing Logic</name>
          <sequenceNumber>1</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[var criticalResults = [];
var normalResults = [];
var abnormalFlags = [];

for each (var obx in msg..OBX) {
  var code = obx['OBX.3']['OBX.3.1'].toString();
  var value = obx['OBX.5']['OBX.5.1'].toString();
  var flag = obx['OBX.8'].toString();
  var units = obx['OBX.6']['OBX.6.1'].toString();

  if (flag === 'H' || flag === 'HH' || flag === 'L' || flag === 'LL') {
    criticalResults.push(code + '=' + value + units + '(' + flag + ')');
    abnormalFlags.push(flag);
  } else {
    normalResults.push(code + '=' + value + units);
  }
}

$c('criticalResults', criticalResults.join('|'));
$c('normalResults', normalResults.join('|'));
$c('abnormalFlagCount', String(abnormalFlags.length));
$c('totalOBXProcessed', String(criticalResults.length + normalResults.length));]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>Dynamic Z-Segment Generation</name>
          <sequenceNumber>2</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[createSegment('ZLR', msg);
msg['ZLR']['ZLR.1'] = $c('totalOBXProcessed');
msg['ZLR']['ZLR.2'] = $c('abnormalFlagCount');
msg['ZLR']['ZLR.3'] = $c('criticalResults');

var hasZPI = msg['ZPI'].length() > 0;
if (hasZPI) {
  var zpiData = msg['ZPI']['ZPI.1'].toString();
  $c('incomingZPI', zpiData);
}
$c('hasIncomingZPI', String(hasZPI));

createSegment('ZAU', msg);
msg['ZAU']['ZAU.1'] = 'NodeJS_Mirth';
msg['ZAU']['ZAU.2'] = new Date().toString();
msg['ZAU']['ZAU.3'] = 'E4X_STRESS_TEST';]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>Repeating Name Handling</name>
          <sequenceNumber>3</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[var names = [];
for (var n = 0; n < msg['PID']['PID.5'].length(); n++) {
  var family = msg['PID']['PID.5'][n]['PID.5.1'].toString();
  var given = msg['PID']['PID.5'][n]['PID.5.2'].toString();
  var nameType = msg['PID']['PID.5'][n]['PID.5.7'].toString();
  names.push(nameType + ':' + family + ',' + given);
}
$c('allNames', names.join('|'));
$c('nameCount', String(names.length));]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>Segment Deletion + Message Cleanup</name>
          <sequenceNumber>4</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[delete msg['NK1'];
delete msg['IN1'];
msg['PID']['PID.19'] = '';

var segCount = 0;
for each (var s in msg.children()) {
  segCount++;
}
$c('finalSegmentCount', String(segCount));
$c('nk1Removed', String(msg['NK1'].length() === 0));
$c('in1Removed', String(msg['IN1'].length() === 0));]]></script>
        </com.mirth.connect.plugins.javascriptstep.JavaScriptStep>
        <com.mirth.connect.plugins.javascriptstep.JavaScriptStep version="3.9.1">
          <name>E4X with Code Template Functions</name>
          <sequenceNumber>5</sequenceNumber>
          <enabled>true</enabled>
          <script><![CDATA[// Call code template library functions (with E4X internally)
try {
  var normalized = normalizePatientNameE4X(msg);
  $c('templateNormalized', normalized);
} catch(e) {
  $c('templateNormalized', 'template_error: ' + e.message);
}

try {
  var auditEntry = buildAuditEntryE4X(msg, 'STRESS_TEST');
  $c('templateAudit', auditEntry);
} catch(e) {
  $c('templateAudit', 'template_error: ' + e.message);
}

$c('e4xStressComplete', 'true');
$c('sourceType', 'E4X_STRESS');]]></script>
